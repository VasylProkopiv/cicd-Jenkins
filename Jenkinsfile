pipeline {
  agent any
  stages {
    stage("checkout") {
      steps {
        echo 'This is a checkout stage'
      }
    }
    stage("build") {
      steps {
        echo 'This is a test stage'
      }
    }
    stage("test") {
      steps {
        echo 'This is a test stage'
      }
    }
    stage("build docker image") {
      steps {
        echo 'This is a test stage'
      }
    }
    stage("deploy") {
      steps {
        echo 'This is a deploy stage'
      }
    }
  }
}
